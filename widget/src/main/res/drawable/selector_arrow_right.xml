<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--选中时的状态-->
    <item android:state_pressed="true" android:drawable="@drawable/arrow_right_checked" />
    <item android:state_checked="true" android:drawable="@drawable/arrow_right_checked" />
    <item android:state_selected="true" android:drawable="@drawable/arrow_right_checked" />
    <!--设置默认图片-->
    <item android:drawable="@drawable/arrow_right_unchecked"/>
</selector>